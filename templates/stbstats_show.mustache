<h1>StB-Statistiken f√ºr {{#season}}{{name}}{{/season}}{{#region_filter}} (nur {{region_filter}}){{/region_filter}}</h1>

<table>
<thead><tr>
	<th>StB</th>
	<th>Staffeln</th>
	<th>Spiele</th>
	<th>Bezirk</th>
	<th>Median</th>
{{#extended}}
	<th>Durchschnitt</th>
{{/extended}}
	<th>95%-Quantil</th>
{{#show_emails}}
	<th>E-Mail</th>
{{/show_emails}}
</tr></thead>
<tbody>
{{#stats}}
<tr><td>{{stb_name}}</td>
	<td class="stbstats_group_count">{{group_count}}</td>
	<td class="stbstats_count">{{count}}</td>
	<td class="stbstats_regions">{{regions_str}}</td>
	<td class="stbstats_median">{{#format_duration_float}}{{median}}{{/format_duration_float}}</td>
{{#extended}}
	<td class="stbstats_avg">{{#format_duration_float}}{{avg}}{{/format_duration_float}}</td>
{{/extended}}
	<td class="stbstats_q95">{{#format_duration_float}}{{q95}}{{/format_duration_float}}</td>
{{#show_emails}}
	<td class="stbstats_email">{{stb_email}}</td>
{{/show_emails}}
</tr>
{{/stats}}

</tbody>
</table>